[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ambition
Group=www-data
WorkingDirectory=/home/ambition/ambition
ExecStart=/home/ambition/.venvs/ambition/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ambition/ambition/gunicorn.blantyre.sock ambition.blantyre.wsgi:application

[Install]
WantedBy=multi-user.target