[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ambition
Group=www-data
WorkingDirectory=/home/ambition/ambition
ExecStart=/home/ambition/.venvs/ambition/bin/gunicorn --access-logfile - --workers 3 --bind unix:/run/gunicorn_kampala.sock ambition.wsgi.kampala:application

[Install]
WantedBy=multi-user.target